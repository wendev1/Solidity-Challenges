 pragma solidity 0.8.7;

 contract Challenge1{
// Four types of Variables
    uint public unsignedintVar;
    bool public booleanVar;
    string public stringVar;
    address public addressVar;

// Set function
    function setUnsignedintVariable(uint256 _newValue) public{
        unsignedintVar = _newValue;
    }

    function setBooleanVar(bool _newValue) public{
        booleanVar = _newValue;
    }

    function setStringVar(string memory _newValue) public{
        stringVar = _newValue;
    }

    function setAddressVar(address _newValue) public{
        addressVar = _newValue;
    }

// Get function for each variable
    function getUnsignedintVar() public view returns (uint256){
        return unsignedintVar;
    }

    function getBooleanVariable() public view returns (bool){
        return booleanVar;
    }

    function getStringVar() public view returns (string memory){
        return stringVar;
    }

    function getAddressVar() public view returns (address){
        return addressVar;
    }
 }
